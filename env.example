# DinoTradez-On-the-Go Environment Configuration
# Copy this file to .env and fill in your actual values

# Finnhub.io API Key (Get free key from https://finnhub.io/register)
FINNHUB_API_KEY=your_finnhub_api_key_here

# Development Settings
NODE_ENV=development
VITE_USE_MOCK=false

# Server Configuration
PORT=3000
API_BASE_URL=http://localhost:3000

# Caching Configuration (in seconds)
CACHE_DURATION_PRICES=300
CACHE_DURATION_NEWS=1800
CACHE_DURATION_FILINGS=3600

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100 